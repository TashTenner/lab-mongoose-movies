<h2>Celebrities</h2>
<ul>
  {{#each celebrities}}
  <li><a href="/celebrities/{{this._id}}">{{ this.name }} </a> </li>
  <form action="/celebrities/{{this._id}}/delete" method="POST">
    {{!-- <input type="submit" value="delete"> del ejemplo de Thor pero no funciona --}}
    <button type="submit">DELETE</button>
  </form>
  <br>
  {{/each}}
</ul>
<a href="/celebrities/new">Add new celebrity</a>


